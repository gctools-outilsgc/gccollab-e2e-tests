{
  "name": "gccollab-e2e-tests",
  "version": "1.9.3",
  "description": "e2e tests for haibun",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "tsc-watch": "tsc --watch",
    "build": "tsc",
    "test": "HAIBUN_O_WEBPLAYWRIGHT_STORAGE=StorageFS HAIBUN_O_WEBPLAYWRIGHT_CAPTURE_VIDEO=true HAIBUN_O_WEBPLAYWRIGHT_HEADLESS=false haibun-cli features",
    "test-chromium": "HAIBUN_SPLIT_SHARED=a-browser=chromium haibun-cli features",
    "test-some": "HAIBUN_O_WEBPLAYWRIGHT_HEADLESS=true HAIBUN_LOOPS=2 HAIBUN_MEMBERS=5 haibun-cli features",
    "test-many": "HAIBUN_O_WEBPLAYWRIGHT_HEADLESS=true HAIBUN_LOOPS=5 HAIBUN_MEMBERS=50 haibun-cli features",
    "test-firefox": "HAIBUN_SPLIT_SHARED=a-browser=firefox haibun-cli features",
    "test-webkit": "HAIBUN_SPLIT_SHARED=a-browser=webkit haibun-cli features",
    "test-features": "HAIBUN_SPLIT_SHARED='a-browser=chromium,webkit,firefox,chromium.iPhone 6,chromium.iPad (gen 7) landscape,chromium.iPad Mini,chromium.Pixel 5,chromium.iPhone X landscape,chromium.Kindle Fire HDX,chromium.Moto G4 landscape' haibun-cli features",
    "test-envc": "HAIBUN_ENVC=what=what1,what=what2,also=also1,also=also2 haibun-cli envc",
    "test-envc-10": "HAIBUN_LOOPS=2 HAIBUN_MEMBERS=5 HAIBUN_ENVC=what=what1,what=what2,also=also1,also=also2 haibun-cli envc",
    "generate-reports": "HAIBUN_O_OUTREVIEWS_TRACE_STORAGE=StorageFS HAIBUN_O_OUTREVIEWS_PUBLISH_STORAGE=StorageFS HAIBUN_O_OUTREVIEWS_INDEX_STORAGE=StorageFS HAIBUN_O_OUTREVIEWS_REVIEWS_STORAGE=StorageFS npm run test-pipeline-indexes"

  },
  "keywords": [],
  "author": "vid",
  "license": "ISC",
  "dependencies": {
    "@haibun/cli": "^1.9.9",
    "@haibun/storage-fs": "^1.9.9",
    "@haibun/domain-storage": "^1.9.9",
    "@haibun/core": "^1.9.9",
    "@haibun/domain-webpage": "^1.9.9",
    "@haibun/web-playwright": "^1.9.9"
  }
}
