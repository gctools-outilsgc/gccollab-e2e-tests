{
  "name": "gccollab-e2e-tests",
  "version": "1.9.3",
  "description": "e2e tests for gccollab",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "cli-help": "haibun-cli features --help",
    "test-public": "HAIBUN_O_OUTREVIEWS_TRACE_STORAGE=StorageFS HAIBUN_TRACE=true HAIBUN_O_WEBPLAYWRIGHT_STORAGE=StorageFS HAIBUN_O_WEBPLAYWRIGHT_CAPTURE_VIDEO=true HAIBUN_O_WEBPLAYWRIGHT_HEADLESS=false haibun-cli haibun/paths/happy,haibun/environments/public,haibun/flows",
    "test-cipher": "HAIBUN_O_OUTREVIEWS_TRACE_STORAGE=StorageFS HAIBUN_TRACE=true HAIBUN_O_WEBPLAYWRIGHT_STORAGE=StorageFS HAIBUN_O_WEBPLAYWRIGHT_CAPTURE_VIDEO=true HAIBUN_O_WEBPLAYWRIGHT_HEADLESS=false haibun-cli haibun/paths/happy,haibun/environments/cipher,haibun/flows",
    "publish": "HAIBUN_O_OUTREVIEWS_TRACE_STORAGE=StorageFS haibun-cli publish",
    "test-chromium": "HAIBUN_O_OUTREVIEWS_TRACE_STORAGE=StorageFS HAIBUN_SPLIT_SHARED=a-browser=chromium haibun-cli features",
    "test-some": "HAIBUN_O_OUTREVIEWS_TRACE_STORAGE=StorageFS HAIBUN_O_WEBPLAYWRIGHT_HEADLESS=true HAIBUN_LOOPS=2 HAIBUN_MEMBERS=5 haibun-cli features",
    "test-many": "HAIBUN_O_WEBPLAYWRIGHT_STORAGE=StorageFS HAIBUN_O_WEBPLAYWRIGHT_CAPTURE_VIDEO=true HAIBUN_O_WEBPLAYWRIGHT_HEADLESS=false HAIBUN_LOOPS=5 HAIBUN_MEMBERS=5 haibun-cli features",
    "test-firefox": "HAIBUN_O_OUTREVIEWS_TRACE_STORAGE=StorageFS HAIBUN_SPLIT_SHARED=a-browser=firefox haibun-cli features",
    "test-webkit": "HAIBUN_O_OUTREVIEWS_TRACE_STORAGE=StorageFS HAIBUN_SPLIT_SHARED=a-browser=webkit haibun-cli features",
    "test-browsers": "HAIBUN_O_OUTREVIEWS_TRACE_STORAGE=StorageFS HAIBUN_MEMBERS=10 HAIBUN_O_WEBPLAYWRIGHT_STORAGE=StorageFS HAIBUN_O_WEBPLAYWRIGHT_CAPTURE_VIDEO=true HAIBUN_O_WEBPLAYWRIGHT_HEADLESS=false iumHAIBUN_MEMBERS=8 HAIBUN_SPLIT_SHARED='a-browser=chromium,webkit,firefox,chromium.iPhone 6,chromium.iPad (gen 7) landscape,chromium.iPad Mini,chromium.Pixel 5,chromium.iPhone X landscape,chromium.Kindle Fire HDX,chromium.Moto G4 landscape' haibun-cli features"
  },
  "keywords": [],
  "author": "vid",
  "license": "ISC",
  "dependencies": {
    "@haibun/cli": "1.21",
    "@haibun/core": "1.21",
    "@haibun/domain-storage": "1.21",
    "@haibun/domain-webpage": "1.21",
    "@haibun/out-review": "1.21",
    "@haibun/storage-fs": "1.21",
    "@haibun/web-playwright": "1.21"
  }
}
