{
  "name": "gccollab-e2e-tests",
  "version": "1.9.3",
  "description": "e2e tests for gccollab",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "cli-help": "haibun-cli features --help",
    "test-public": ". ./env-default.sh && haibun-cli $PUBLIC_ALL",
    "test-public-happy": ". ./env-default.sh && HAIBUN_OUTPUT_DEST=junit/public-happy.xml haibun-cli $PUBLIC_HAPPY",
    "test-public-a11y": ". ./env-default.sh && HAIBUN_OUTPUT_DEST=junit/public-a11y.xml haibun-cli $PUBLIC_A11Y",
    "test-public-regressions": ". ./env-default.sh && HAIBUN_OUTPUT_DEST=junit/public-regressions.xml haibun-cli $PUBLIC_REGRESS",
    "test-pr": ". ./env-default.sh && haibun-cli $PR_ALL",
    "test-pr-happy": ". ./env-default.sh && . ./env-azure.sh && HAIBUN_OUTPUT_DEST=junit/pr-happy.xml haibun-cli $PR_HAPPY",
    "test-pr-a11y": ". ./env-default.sh && HAIBUN_OUTPUT_DEST=junit/pr-a11y.xml haibun-cli $PR_A11Y",
    "test-pr-regressions": ". ./env-default.sh && . ./env-azure.sh && HAIBUN_OUTPUT_DEST=junit/pr-regressions.xml haibun-cli $PR_REGRESS",
    "test-cipher": ". ./env-default.sh && haibun-cli $CIPHER_ALL",
    "test-cipher-happy": ". ./env-default.sh && haibun-cli HAIBUN_OUTPUT_DEST=junit/cipher-happy.xml $CIPHER_HAPPY",
    "test-cipher-regressions": ". ./env-default.sh && haibun-cli HAIBUN_OUTPUT_DEST=junit/cipher-regressions.xml $CIPHER_REGRESS",
    "publish-local-captures-indexes": "HAIBUN_O_OUTREVIEWS_TRACE_STORAGE=StorageFS HAIBUN_O_OUTREVIEWS_PUBLISH_STORAGE=StorageFS HAIBUN_O_OUTREVIEWS_INDEX_STORAGE=StorageFS HAIBUN_O_OUTREVIEWS_REVIEWS_STORAGE=StorageFS haibun-cli --config publish/local publish",
    "publish-azure-captures-indexes": "HAIBUN_O_OUTREVIEWS_TRACE_STORAGE=StorageFS HAIBUN_O_OUTREVIEWS_PUBLISH_STORAGE=AzureStorageBlob HAIBUN_O_OUTREVIEWS_INDEX_STORAGE=StorageFS HAIBUN_O_OUTREVIEWS_REVIEWS_STORAGE=AzureStorageBlob HAIBUN_O_AZURESTORAGEBLOB_DESTINATION=${_O_AZURESTORAGEBLOB_DESTINATION} HAIBUN_O_AZURESTORAGEBLOB_ACCOUNT=${_O_AZURESTORAGEBLOB_ACCOUNT} HAIBUN_O_AZURESTORAGEBLOB_KEY=${_O_AZURESTORAGEBLOB_KEY} HAIBUN_O_OUTREVIEWS_URI_ARGS=${_O_OUTREVIEWS_URI_ARGS} haibun-cli --config publish/azure publish",
    "clear-local-captures": "haibun-cli --config clear/local clear",
    "clear-azure-captures": "HAIBUN_O_AZURESTORAGEBLOB_DESTINATION=${_O_AZURESTORAGEBLOB_DESTINATION} HAIBUN_O_AZURESTORAGEBLOB_ACCOUNT=${_O_AZURESTORAGEBLOB_ACCOUNT} HAIBUN_O_AZURESTORAGEBLOB_KEY=${_O_AZURESTORAGEBLOB_KEY} haibun-cli --config clear/azure clear",
    "azure-storage": "npm run clear-azure-captures && npm run test-public ; npm run publish-azure-captures-indexes",
    "test-chromium": ". ./env-default.sh && HAIBUN_SPLIT_SHARED=a-browser=chromium haibun-cli $CIPHER_ALL",
    "test-some": ". ./env-default.sh && HAIBUN_LOOPS=2 HAIBUN_MEMBERS=5 haibun-cli $CIPHER_ALL",
    "test-many": ". ./env-default.sh && HAIBUN_LOOPS=5 HAIBUN_MEMBERS=5 haibun-cli $CIPHER_ALL",
    "test-firefox": ". ./env-default.sh && HAIBUN_SPLIT_SHARED=a-browser=firefox haibun-cli $CIPHER_ALL",
    "test-webkit": ". ./env-default.sh && HAIBUN_SPLIT_SHARED=a-browser=webkit haibun-cli $CIPHER_ALL",
    "test-browsers": ". ./env-default.sh && HAIBUN_MEMBERS=10 HAIBUN_MEMBERS=8 HAIBUN_SPLIT_SHARED='a-browser=chromium,webkit,firefox,chromium.iPhone 6,chromium.iPad (gen 7) landscape,chromium.iPad Mini,chromium.Pixel 5,chromium.iPhone X landscape,chromium.Kindle Fire HDX,chromium.Moto G4 landscape' haibun-cli $CIPHER_ALL"
  },
  "keywords": [],
  "author": "vid",
  "license": "ISC",
  "dependencies": {
    "@haibun/azure": "^1.25.2",
    "@haibun/cli": "1.25.1",
    "@haibun/core": "1.25",
    "@haibun/domain-storage": "1.25",
    "@haibun/domain-webpage": "1.25",
    "@haibun/out-review": "1.25",
    "@haibun/out-xunit": "^1.25.0",
    "@haibun/storage-fs": "1.25",
    "@haibun/web-accessibility-axe": "^1.22.0",
    "@haibun/web-playwright": "1.25"
  }
}
